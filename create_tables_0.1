CREATE TABLE palabra (
	palabra_id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL ,
	title TEXT,
	pronunciation TEXT,
	definition TEXT,
	source TEXT,
	grade_of_knowledge INTEGER,
	priority INTEGER,
	example TEXT,
	fk_language_id INTEGER,
	FOREIGN KEY(fk_language_id)  REFERENCES language (language_id)
);

